{
    "Logging": {
        "File": {
            "Files": [
                {
                    "Path": "default-<counter>.log",
                    "MinLevel": {
                        "Default": "Error"
                    }
                }
            ]
        },
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "Hangfire": "Information",
            "npgsql.application": "Trace",
            //"npgsql.other": "Trace",
            "LandscapeRegistry.Runners.CLBRunner": "Information"
        }
    },
    "Authentication": {
        "Authority": "https://host.docker.internal:8443/auth/realms/landscape",
        "Audience": "landscape-registry"
    },
    "Keycloak": {
        "URL": "https://host.docker.internal:8443"
    },
    "CORS": {
        "AllowedHosts": "http://localhost:3000,https://localhost:3000"
    },
    "BaseURL": ""
}
