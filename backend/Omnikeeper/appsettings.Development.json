      "Omnikeeper.Runners.ExternalIDManagerRunner": "Debug"
      //"Omnikeeper.Runners.ArchiveOldDataRunner": "Debug",
      //"Omnikeeper.Base.Model.IODataAPIContextModel": "Debug"
    }
  },
  "Authentication": {
    "Authority": "https://host.docker.internal:8443/auth/realms/landscape",
    "Audience": "landscape-omnikeeper",
    "debugAllowAll": false
  },
  "Authorization": {
    "debugAllowAll": true
  },
  "Keycloak": {
    "URL": "https://host.docker.internal:8443",
    "Realm": "landscape",
    "ClientID": "landscape-omnikeeper-api",
    "ClientSecret": "2e9d8aab-5286-4c6b-a8d5-2b3534f45a04"
