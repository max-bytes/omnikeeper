{
  "DetailedErrors": true,
  "Logging": {
    "File": {
      "Files": [
        {
          "Path": "app-<counter>.log",
          "MinLevel": {
            "Default": "Error"
          }
        }
      ]
    },
    "PathFormat": "Logs/log-{Date}.log",
    "FileSizeLimitBytes": 104857600,
    "RetainedFileCountLimit": 30,
    "Json": true,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      //"Microsoft.Hosting.Lifetime": "Information",
      //"Hangfire": "Information",
      //"npgsql.application": "Trace",
      //"npgsql.other": "Trace",
      //"Omnikeeper.Runners.CLBRunner": "Debug"
