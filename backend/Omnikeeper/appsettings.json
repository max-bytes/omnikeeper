{
  "Logging": {
    "PathFormat": "Logs/app-{Date}.log",
    "FileSizeLimitBytes": 10485760, // =10mb
    "RetainedFileCountLimit": 30,
    "Json": true,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Hangfire": "Information"
    }
  },
  "Authentication": {
    "debugAllowAll": false
  },
  "Authorization": {
    "debugAllowAll": false
  },
  "BaseURL": "",
  "ShowPII": false
}
